.s-content-page
  position relative
  top 0
  left 0
  z-index 8

  display inline-block
  width 100%
  height 100%
  margin 0
  padding 0 0 header-height menu-width
  box-sizing border-box

  font-size 0

  @media media-tablet
    margin 0
    padding 0

  @media media-phone
    margin 0
    padding 0

  &-inner-wrapper
    width 100%
    height 100%
    background-color #FF0000
